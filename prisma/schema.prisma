generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "sqlite"
  url      = env("DATABASE_URL")
}



model User {
  id Int @id @default(autoincrement())
  name String 
  password String 
  created_at DateTime @default(now())
  updated_at DateTime @updatedAt

  @@map("users")
}